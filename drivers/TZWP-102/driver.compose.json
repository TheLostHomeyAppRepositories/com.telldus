      "title": {
        "en": "Reset power meter"
      },
      "desc": {
        "en": "Reset the accumulated power usage (kWh), note that this can not be reversed."
      }
    }
  },
  "images": {
    "large": "/drivers/TZWP-102/assets/images/large.png",
